<div id="<%= dom_id artist %>" class="card mb-4 shadow-sm" style="width: 18rem;">
  <img src="<%= artist.photo %>" class="card-img-top" alt="<%= artist.name %>">
  <div class="card-body">
    <h5 class="card-title"><%= artist.name %></h5>
    <p class="card-text"><%= artist.description %></p>
  </div>
</div>